generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_URL")
}

model expert_linkedin_queue {
  id           Int       @id @default(autoincrement())
  url          String?   @unique @db.VarChar(255)
  status       Int?      @default(0)
  result       Json?
  last_scrape  DateTime? @db.Timestamp(0)
  processed    Int?      @default(0)
  last_process DateTime? @db.Timestamp(0)
  expert_id    Int?
  created_at   DateTime? @default(now()) @db.Timestamp(0)
  updated_at   DateTime? @default(now()) @db.Timestamp(0)
  experts      experts?  @relation(fields: [expert_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([expert_id])
}

model activity_log {
  id           Int       @id @default(autoincrement())
  log_name     String?   @db.VarChar(255)
  description  String    @db.Text
  subject_type String?   @db.VarChar(255)
  event        String?   @db.VarChar(255)
  subject_id   Int?
  causer_type  String?   @db.VarChar(255)
  causer_id    Int?
  properties   Json?
  batch_uuid   String?   @db.Char(36)
  created_at   DateTime? @db.Timestamp(0)
  updated_at   DateTime? @db.Timestamp(0)

  @@index([log_name])
  @@index([causer_type, causer_id])
  @@index([subject_type, subject_id])
}

model assessment {
  id         Int       @id @default(autoincrement())
  user_id    Int?
  answered   Json?
  start_at   DateTime? @db.Timestamp(0)
  score      Int?
  total      Int?
  status     String?   @db.VarChar(50)
  created_at DateTime? @default(now()) @db.Timestamp(0)
  updated_at DateTime? @default(now()) @db.Timestamp(0)
  users      users?    @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([user_id])
}

model assessment_questions {
  id         Int       @id @default(autoincrement())
  order      Int?
  question   String?   @db.Text
  options    Json?
  created_at DateTime? @default(now()) @db.Timestamp(0)
  updated_at DateTime? @default(now()) @db.Timestamp(0)
}

model assessment_timers {
  id          Int       @id @default(autoincrement())
  total_timer DateTime? @db.Time(0)
  created_at  DateTime? @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)
}

model authentication_log {
  id                   Int       @id @default(autoincrement())
  authenticatable_type String    @db.VarChar(255)
  authenticatable_id   Int
  ip_address           String?   @db.VarChar(45)
  user_agent           String?   @db.Text
  login_at             DateTime? @db.Timestamp(0)
  login_successful     Boolean   @default(false)
  logout_at            DateTime? @db.Timestamp(0)
  cleared_by_user      Boolean   @default(false)
  location             Json?

  @@index([authenticatable_type, authenticatable_id])
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model career_categories {
  id            Int       @id @default(autoincrement())
  category_type String?   @db.VarChar(255)
  slug          String?   @db.VarChar(255)
  status        String?   @db.VarChar(255)
  created_at    DateTime? @db.Timestamp(0)
  updated_at    DateTime? @db.Timestamp(0)
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model career_jobs {
  id                           Int       @id @default(autoincrement())
  career_sub_categories_tbl_id Int
  title                        String?   @db.VarChar(255)
  heading                      String?   @db.VarChar(255)
  discretion                   String?   @db.Text
  image                        String?   @db.VarChar(255)
  slug                         String?   @db.VarChar(255)
  status                       String?   @db.VarChar(255)
  created_at                   DateTime? @db.Timestamp(0)
  updated_at                   DateTime? @db.Timestamp(0)
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model career_sub_categories {
  id                       Int       @id @default(autoincrement())
  career_categories_tbl_id Int
  name                     String?   @db.VarChar(255)
  slug                     String?   @db.VarChar(255)
  status                   String?   @db.VarChar(255)
  created_at               DateTime? @db.Timestamp(0)
  updated_at               DateTime? @db.Timestamp(0)
}

model chat_messages {
  id         Int       @id @default(autoincrement())
  project_id Int?
  user_id    Int?
  message    String?   @db.Text
  created_at DateTime? @default(now()) @db.Timestamp(0)
  updated_at DateTime? @default(now()) @db.Timestamp(0)
  users      users?    @relation(fields: [user_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  projects   projects? @relation(fields: [project_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([project_id])
  @@index([user_id])
}

/// This model or at least one of its fields has comments in the database, and requires an additional setup for migrations: Read more: https://pris.ly/d/database-comments
model cities {
  id           Int       @id @default(autoincrement())
  name         String    @db.VarChar(255)
  state_id     Int
  state_code   String    @db.VarChar(255)
  country_id   Int
  country_code String    @db.Char(2)
  latitude     Decimal   @db.Decimal(10, 8)
  longitude    Decimal   @db.Decimal(11, 8)
  created_at   DateTime  @default(now()) @db.Timestamp(0)
  updated_at   DateTime  @default(now()) @db.Timestamp(0)
  flag         Boolean   @default(true)
  wikiDataId   String?   @db.VarChar(255)
  states       states    @relation(fields: [state_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  countries    countries @relation(fields: [country_id], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@index([state_id])
  @@index([country_id])
}

model client {
  id         Int        @id @default(autoincrement())
  user_id    Int?
  company_id Int?
  team_id    Int?
  created_at DateTime?  @default(now()) @db.Timestamp(0)
  updated_at DateTime?  @default(now()) @db.Timestamp(0)
  users      users?     @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  companies  companies? @relation(fields: [company_id], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@index([company_id])
  @@index([user_id])
}

model client_team {
  id         Int        @id @default(autoincrement())
  name       String?    @db.VarChar(255)
  company_id Int?
  created_by Int?
  created_at DateTime?  @default(now()) @db.Timestamp(0)
  updated_at DateTime?  @default(now()) @db.Timestamp(0)
  companies  companies? @relation(fields: [company_id], references: [id], onUpdate: NoAction)
  users      users?     @relation(fields: [created_by], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([company_id])
  @@index([created_by])
}

model companies {
  id              Int               @id @default(autoincrement())
  name            String?           @db.VarChar(255)
  linkedin_vanity String?           @db.VarChar(255)
  slogan          String?           @db.Text
  website         String?           @db.VarChar(255)
  type_id         Int?
  establish       Int?
  company_size    String?           @db.VarChar(255)
  about           String?           @db.Text
  industry_id     String?           @db.VarChar(100)
  status          String?           @db.VarChar(50)
  img_url         String?           @db.Text
  specialties     Json?
  others          Json?
  pic             Json?
  created_by      Int?
  created_at      DateTime?         @default(now()) @db.Timestamp(0)
  updated_at      DateTime?         @default(now()) @db.Timestamp(0)
  client          client[]
  client_team     client_team[]
  company_type    company_type?     @relation(fields: [type_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
  users           users?            @relation(fields: [created_by], references: [id], onDelete: NoAction, onUpdate: NoAction)
  company_address company_address[]
  company_finance company_finance[]
  company_staff   company_staff[]
  projects        projects[]
  projects_old    projects_old[]

  @@index([type_id])
  @@index([created_by])
}

model company_address {
  id         Int        @id @default(autoincrement())
  company_id Int?
  address    String?    @db.Text
  city       String?    @db.VarChar(255)
  postal     String?    @db.VarChar(255)
  state      String?    @db.VarChar(255)
  country    String?    @db.VarChar(255)
  emoji      String?    @db.VarChar(10)
  created_at DateTime?  @default(now()) @db.Timestamp(0)
  updated_at DateTime?  @default(now()) @db.Timestamp(0)
  companies  companies? @relation(fields: [company_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "company_address_ibfk_1")

  @@index([city])
  @@index([company_id])
  @@index([country])
  @@index([state])
}

model company_finance {
  id                     Int        @id @default(autoincrement())
  company_id             Int?
  revenue                Int?
  operating_profit       Int?
  net_profit             Int?
  total_assets           Int?
  current_market_capital Int?
  capital                Int?
  created_at             DateTime?  @default(now()) @db.Timestamp(0)
  updated_at             DateTime?  @default(now()) @db.Timestamp(0)
  companies              companies? @relation(fields: [company_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([company_id])
}

model company_staff {
  id           Int        @id @default(autoincrement())
  company_id   Int?
  name         String?    @db.VarChar(255)
  position     String?    @db.VarChar(255)
  phone        String?    @db.VarChar(255)
  email        String?    @db.VarChar(255)
  linkedin_url String?    @db.VarChar(255)
  is_pic       Boolean?
  updated_at   DateTime?  @default(now()) @db.Timestamp(0)
  created_at   DateTime?  @default(now()) @db.Timestamp(0)
  companies    companies? @relation(fields: [company_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([company_id])
}

model company_type {
  id         Int         @id @default(autoincrement())
  name       String?     @db.VarChar(255)
  created_at DateTime?   @default(now()) @db.Timestamp(0)
  updated_at DateTime?   @default(now()) @db.Timestamp(0)
  companies  companies[]
}

model contract_expert {
  id                 Int                @id @default(autoincrement())
  project_expert_id  Int?
  contract_id        String?            @db.VarChar(255)
